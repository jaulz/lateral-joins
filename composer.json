{
    "name": "jaulz/lateral-joins",
    "description": "Extends Laravel query builder with lateral joins",
    "homepage": "https://www.hundeloh-consulting.ch",
    "license": "MIT",
    "authors": [
        {
            "name": "Julian Hundeloh",
            "email": "julian@hundeloh-consulting.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^9.0",
        "illuminate/database": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Jaulz\\LateralJoins\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Jaulz\\LateralJoins\\DatabaseServiceProvider"
            ]
        }
    }
}
